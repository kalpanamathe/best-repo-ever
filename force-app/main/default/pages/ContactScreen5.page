<apex:page controller="ContactPagenation" renderAs="PDF">
    <apex:form >
         <apex:pageBlock title="Contact Edit" helpTitle="Help" helpurl="https://www.google.com" tabStyle="Contact"> 
            <apex:pageBlockSection title="Contact Information" columns="2">
                <apex:OutputText value="{!con.Name}"/> 
                <apex:OutputText value="{!con.Phone}"/> 
                <apex:OutputText value="{!con.Salutation}"/> 
                <apex:OutputText value="{!con.FirstName}"/> 
                <apex:OutputText value="{!con.LastName}"/> 
                <apex:OutputText value="{!con.AccountId}"/> 
                <apex:OutputText value="{!con.MobilePhone}"/> 
                <apex:OutputText value="{!con.Title}"/> 
                <apex:OutputText value="{!con.OtherPhone}"/> 
                <apex:OutputText value="{!con.Department}"/> 
                <apex:OutputText value="{!con.Fax}"/> 
                <apex:OutputText value="{!con.Birthdate}"/> 
                <apex:OutputText value="{!con.Email}"/> 
                <apex:OutputText value="{!con.ReportsToId}"/> 
                <apex:OutputText value="{!con.AssistantName}"/> 
                <apex:OutputText value="{!con.LeadSource}"/> 
                <apex:OutputText value="{!con.AssistantPhone}"/> 
                <apex:OutputText value="{!con.Gender__c}"/> 
                <apex:OutputText value="{!con.Receive_Email_or_not__c}"/> 
                <apex:OutputText value="{!con.TimeZone__c}"/> 
                <apex:OutputText value="{!con.Twitter_ID__c}"/> 
                <apex:OutputText value="{!con.primary_contact__c}"/> 
            </apex:pageBlockSection>  
            
            <apex:pageBlockSection title="Address Information" columns="2" >
                <apex:inputField value="{!con.MailingStreet}"    /> 
                <apex:OutputText value="{!con.OtherStreet}"/> 
            </apex:pageBlockSection> 
            
            <apex:pageBlockSection >
                <apex:OutputText value="{!con.MailingCity}"/> 
                <apex:OutputText value="{!con.OtherCity}"/> 
                <apex:OutputText value="{!con.MailingState}"/> 
                <apex:OutputText value="{!con.OtherState}"/> 
                <apex:OutputText value="{!con.MailingPostalCode}"/>
                <apex:OutputText value="{!con.OtherPostalCode}"/>
                <apex:OutputText value="{!con.MailingCountry}"/> 
                <apex:OutputText value="{!con.OtherCountry}"/> 
            </apex:pageBlockSection> 
            
            <apex:pageBlockSection title="Additional Information" columns="2" >
                <apex:OutputText value="{!con.Languages__c}"    /> 
                <apex:OutputText value="{!con.Level__c}"/> 
            </apex:pageBlockSection> 
            
            <apex:pageBlockSection title="Description Information" columns="1" >
                <apex:OutputText value="{!con.Description}"    /> 
                <apex:OutputText value="{!con.Language__c  }"    /> 
            </apex:pageBlockSection> 
            
         
        </apex:pageBlock>
    </apex:form>
</apex:page>